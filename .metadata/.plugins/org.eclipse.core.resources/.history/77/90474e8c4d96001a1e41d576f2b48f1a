package ExerciciosFixacao;

public class Ex3 {
	public static void main (String args[]) {
		
		int i = 223;
		while (i <= 456) {
			System.out.println("contagem nº1: " + i);
			System.out.println("-------------------");
			i = i + 5;
			
				if (i == 300) {
					while (i <= 400) {
						System.out.println("Contagem nº2: " + i);
						System.out.println("-------------------");
						i = i + 3;
					}
			}
		}
		
	}
}
