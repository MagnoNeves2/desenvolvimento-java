package ExerciciosFixacao;

public class Ex3 {
	public static void main (String args[]) {
		
		
		for (int i = 223; i <= 456; i += 5) {
			while (i >= 300 && i <= 400) {
				System.out.println(i += 3);
			}
			System.out.println(i);
				
		}
		
		System.out.println("==============//==============");
		
		for(int i = 223; i <= 456; i += 5) {
			if(i >= 300 && i <= 400) {
				i-=2;
			}
			System.out.println(i);
		}
		
	}
}
